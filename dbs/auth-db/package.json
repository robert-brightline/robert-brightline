{
  "name": "@robert-brightline/auth-db",
  "description": "- [ ] add project description",
  "version": "0.0.10",
  "author": {
    "name": "robert-brightline",
    "email": "robert.brightline+auth-db@gmail.com"
  },
  "homepage": "https://github.com/robert-brightline/robert-brightline.git/dbs/auth-db",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "keywords": [
    "database"
  ],
  "funding": [
    {
      "type": "cashapp",
      "url": "https://cash.app/$puqlib"
    }
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@robert-brightline/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/prisma/client.d.ts",
      "import": "./dist/prisma/client.js",
      "default": "./dist/prisma/client.js"
    },
    "./browser": {
      "types": "./dist/prisma/browser.d.ts",
      "import": "./dist/prisma/browser.js",
      "default": "./dist/prisma/browser.js"
    },
    "./models": {
      "types": "./dist/prisma/models.d.ts",
      "import": "./dist/prisma/models.js",
      "default": "./dist/prisma/models.js"
    },
    "./enums": {
      "types": "./dist/prisma/enums.d.ts",
      "import": "./dist/prisma/enums.js",
      "default": "./dist/prisma/enums.js"
    },
    "./zod": {
      "types": "./dist/zod/index.d.ts",
      "import": "./dist/zod/index.js",
      "default": "./dist/zod/index.js"
    }
  },
  "files": [
    "dist",
    "assets",
    "prisma",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "dbs/auth-db/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dbs/auth-db/dist",
          "main": "dbs/auth-db/src/index.ts",
          "tsConfig": "dbs/auth-db/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      },
      "prisma-generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npx prisma generate",
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "peerDependencies": {
    "@prisma/client": "7.2.0",
    "zod": "4.3.4",
    "@robert-brightline/zod": "workspace:*"
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11"
  },
  "devDependencies": {
    "@robert-brightline/zod": "workspace:*",
    "dotenv": "^17.2.3",
    "prisma": "^7.2.0",
    "vitest": "^4.0.8"
  }
}